    function setACT(objAdobe)//set all properties and Events that needs to tarcked in the custom tracking
    {
        if (objAdobe != null) {//IF user input is null then don't tarck anything
            if (null != s_account) {
                var s = s_gi(s_account);
                var properties = "";
                var vars = "";
                if (objAdobe.events != null) {
                    s.events = objAdobe.events;
                    //Sets(link) the events that are tarcking in current context.
                    s.linkTrackEvents = objAdobe.events;
                }
                else
                    s.events = "";
                if (objAdobe.prop28 != null) {
                    s.prop28 = objAdobe.prop28;
                    properties += "prop28,";
                }
                //Add all new variables/ parameters if required

                //----Don't alter below lines ofcode --------------------------------------------//
                if (vars.length > 0)
                    vars = vars.substring(0, vars.length - 1);

                if (properties.length > 0)
                    properties = properties.substring(0, properties.length - 1);

                var events = objAdobe.events != null && objAdobe.events.length;//Boolean value
                //Add events if any exists in the current context
                if (events)
                    s.linkTrackVars = s.linkTrackVars.length > 0 ? s.linkTrackVars + "," + "events" : "events";//"events";

                //Add eVars if any exists in the current context
                if (vars.length > 0)
                    s.linkTrackVars = s.linkTrackVars.length > 0 ? s.linkTrackVars + "," + vars : vars;//s.linkTrackVars + "," + vars;

                //Add properties if any exists in the current context
                if (properties.length > 0)
                    s.linkTrackVars = s.linkTrackVars.length > 0 ? s.linkTrackVars + "," + properties : properties;
                if (objAdobe.products != null && objAdobe.products.length > 0)
                    s.linkTrackVars = s.linkTrackVars.length > 0 ? s.linkTrackVars + ",products" : "products";

                //Sets(link) the events that are tarcking in current context.
                s.linkTrackEvents = objAdobe.events;

                s.tl(this, "o", null);

                //---------------------------------------------------------------------------------------------------------
            }
        }
        // return false;
    }



</script>

