<script type="text/javascript">
    function goClick() {
        if (Page_ClientValidate("MobileNumber") == true) {
            var mobileNumber = $('#' + 'ctl00_PlaceHolderMain_Home_txtMobileNumber').val();
            helloBoxGo(mobileNumber);
        }
        return false;
    }
    function helloBoxGo(mobileNumber) {
        $(".showloadingHello").show();
        $(".showloadingHello").attr("src", "/_layouts/15/images/consumer/loading.gif");
        $.ajax({
            type: "Post",
            url: '/_layouts/15/handlers/Home.ashx',
            dataType: "text",
            data: { 'MobileNumber': mobileNumber, 'Event': 'HelloBox' },
            success: helloBoxGoResult,
            error: onFail
        });
    }
    function helloBoxGoResult(result) {
        hideall();
        $(".showloadingHello").hide();
        var typecheck = result.split("|");
        if (typecheck.length > 0) {
            switch (typecheck[0]) {
                case 'Prepaid': {
                      window.location = typecheck[1];
                      break;
                  }
                  case 'Postpaid': {
                      window.location = typecheck[1];
                      break;
                  }
                  case 'NVF': {
                      $("#divSwitch").show();
                      $("#divSwitchPopOverlay").show();
                      break;
                  }
                  case 'ERROR!!!': {
                      $("#divError").show();
                      $("#divSwitchPopOverlay").show();
                      break;
                  }
                  case 'VERROR': {
                      window.location = '/_layouts/error.aspx';
                    break;
                }
                default: {
                    $("#divError").show();
                    $("#divSwitchPopOverlay").show();
                    break;
                }
            }
        }
    }
    function onFail(result) {
        $("#divError").show();
        //$("#divSwitchPopOverlay").show();
    }
    function hideall() {
        $("#divError").hide();
        $("#divSwitchPopOverlay").hide();
        $("#divSwitch").hide();
    }
    var event1 = "";
    function checkandSubmitCaroselRqst(mobileNumber, eventFrom) {
        event1 = eventFrom;
        $.ajax({
            type: "Post",
            url: '/_layouts/15/handlers/Home.ashx',
            dataType: "text",
            data: { 'MobileNumber': mobileNumber, 'Event': eventFrom },
            success: caroselReponse,
            error: onFail
        });
    }
    function showErrorPopup() { $(".divError, .overlay").show() }

    function caroselReponse(result) {
        var o = result.split("|");
        switch (event1) {
            case "HmPrepaidPacks":
                return void ("VF PREPAID" == o[0] ? window.location = o[1] : "VF POSTPAID" == o[0] ? ($(".showloading1").hide(), $(".divpp .screenz").hide(), $(".divpp .screen3").show()) : "ERROR!!!" == o[0] ? (showErrorPopup(), $(".showloading1").hide()) : ($(".showloading1").hide(), $(".divpp .screenz").hide(), $(".divpp .screen2").show()));
            case "HmMobileInternet":
                return void ("VF PREPAID" == o[0] || "VF POSTPAID" == o[0] ? window.location = o[1] : "ERROR!!!" == o[0] ? (showErrorPopup(), $(".showloading2").hide()) : ($(".showloading2").hide(), $(".divmi .screenz").hide(), $(".divmi .screen2").show()));
            case "HmRoamingPacks":
                return void ("VF PREPAID" == o[0] || "VF POSTPAID" == o[0] ? window.location = o[1] : "ERROR!!!" == o[0] ? (showErrorPopup(), $(".showloading3").hide()) : ($(".showloading3").hide(), $(".divir .screenz").hide(), $(".divir .screen2").show()))
        }
    }
    function getNumDetails(i, e) {
        var o = $(i).attr("rel");
        if (Page_ClientValidate(o)) {
            s = $(i).parent().parent().find(".showloadings");
