<script type="text/javascript">
    var mcal = 1;
    $(document).ready(function () {
        $(".nav-close").click(function () {
            $(".subMenu").hide();
        });
        $(".menu").click(function () {
            getMenu();
        });
    });
    function getMenu() {
        if (mcal == 0) {
            mcal = 1;
            $.ajax({
                type: "Get",
                url: '/_layouts/15/handlers/Navigation.ashx',
                dataType: "text",
                success: menuSuccess,
                error: menuFail
            });
        }
    }
    function menuSuccess(result) {
        var resultSet = JSON.parse(result);
        switch (resultSet.status) {
            case "success": $("#ctl00_TM_divMid").html("").html(resultSet.menuContent); menuclicks(); setMenuSripts(); break;
            case "error": window.location = '/_layouts/error.aspx'; break;
            default: alert('Sorry, there was a problem. Please try after some time.'); break;
        }
    }
    function setMenuSripts() {
        if (/Android|iPhone|iPad|iPod|BlackBerry|Opera Mini/i.test(navigator.userAgent)) {
            $('.menubar .nav > li > a').click(function () { $(this).parent().toggleClass('selected').siblings('li').find('a').parent().removeClass('selected'); if ($(this).parent().hasClass('selected')) { $(this).parent().find('.subMenu').stop().slideDown().parent().siblings('li').find('.subMenu').slideUp(); } else { $('.subMenu').stop().hide(); } });
        }
        else {
            $('.menubar .nav > li').hover(function () {
                $(this).toggleClass('selected').siblings('li').removeClass('selected');
                if ($(this).hasClass('selected')) {
                    $(this).find('.subMenu').stop().slideDown();
                } else {
                    $('.subMenu').stop().hide();
                }
            });
        }
    }
    function menuFail(result) { mcal = 0; }
</script>

