	$(document).keydown(function(event) {
		if(event.which  == 38 || event.which  == 33){event.preventDefault();scrollpageup()}	
		else if(event.which  == 40 || event.which  == 34){event.preventDefault();scrollpagedown()}
	});
	$(document).bind('mousewheel DOMMouseScroll MozMousePixelScroll', function(event) {
	    event.preventDefault();
	    var delta = event.originalEvent.wheelDelta || -event.originalEvent.detail;
		init_scroll(event, delta);
	});
	var lastAnimation =0;
	function init_scroll(event, delta) {
	   var timeNow = new Date().getTime();
	   if(timeNow - lastAnimation < 1000) {event.preventDefault();return;}
	   if (delta < 0) {scrollpagedown()} 
	   else {scrollpageup()}
	   lastAnimation = timeNow;
	}
	function resizeContent() {
	   $height = $(window).height();
	   $('body div#pageHeight').height($height);
	}
	function scrollpagedown(){$('html, body').animate({scrollTop: $(".demo-wrapper").offset().top}, 1000);}
	function scrollpageup(){$('html, body').animate({scrollTop: $(".active").offset().top}, 1000);}		
