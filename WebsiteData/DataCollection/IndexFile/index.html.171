var contener=$(".cricket-widget .snmatch ul");
var count=0;
$('.scoreWidgetTab li').on('click', function(e){
	e.preventDefault();
	var dataSlug = $(this).data('slug');
	$(this).addClass('act').siblings().removeClass('act');
	$('#scoreWedgetBox ul').hide();
	$('#'+dataSlug).show();
	if(dataSlug=="matchResultBox"){
		var matchlength = $(this).attr('matchlength');
		if(matchlength>0){
			$('.scoreArrow').show();
		}else{
			$('.scoreArrow').hide();
		}
		
	}else{
		$('.scoreArrow').show();
	}
	$('.scoreArrow').attr('tab_ul_id', dataSlug);
	var count=0;
	$('#scoreWedgetBox ul').animate({marginLeft:'0'}, 500);
	
});

var li_width=contener.find("li").width();
var totel_li=contener.find("li").length; /* total li contener width*/
//alert(total_lwidth);
$("a.scoreArrow").click(function(e){
	var dataSlugCl = $(this).attr('tab_ul_id');
	var slContener = $('#'+dataSlugCl);
	var totalLi = slContener.find("li").length;
	var li_width=contener.find("li").width();
	//console.log(count);
	e.preventDefault();
	var dir=$(this).data("dir")
	if(dir=="right"){
	  count=count+1;
	}
	else{
	  count=count-1;
	}
	if(count <0 || count>=totalLi){
		count=0;
	}
	slContener.animate({marginLeft:'-'+count*li_width+'px'}, 500);
});
	  
$('.cricket-widget ul.cwtab li').on('click', function(e){
  e.preventDefault();
  $(this).addClass('act').siblings().removeClass('act');
});
</script>

<script>
var ibnliveSiteUrl="http://www.news18.com/";
var teamsstring ="Australia~AUS,Bangladesh~BAN,England~ENG,India~IND,New Zealand~NZ,Pakistan~PAK,South Africa~SA,Sri Lanka~SL,West Indies~WI,Zimbabwe~ZIM,Bermuda~BEM,Canada~CAN,East Africa~EA,Hong Kong~HK,Ireland~IRE,Kenya~KEN,Scotland~SCO,United Arab Emirates~UAE,Netherlands~NED,Namibia~NAM,Afghanistan~AFG,Central Stags~CS,Guyana~GUY,Highveld Lions~HL,Mumbai Indians~MI,Southern Redbacks~SRB,Victorian Bushrangers~VB,Warriors~WAR,Wayamba Elevens~WE,Chennai Super Kings~CSK,Kolkata Knight Riders~KKR,Deccan Chargers~DC,Rajasthan Royals~RR,Kochi Tuskers Kerala~KTK,Royal Challengers Bangalore~RCB,Mumbai Indians~MI,Delhi Daredevils~DD,Pune Warriors India~PWI,Kings XI Punjab~KXP,Sunrisers Hyderabad~SRH,Auckland Aces~AA,Somerset~SOM,Leicestershire Foxes~LEI,Trinidad and Tobago~TT,Ruhunu Eleven~RUH,Warriors~WAR,Cape Cobras~CC,New South Wales Blues~NSW,South Australia Redbacks~SAR,Sialkot Stallions~ST,Auckland Aces~AA,Hampshire~HAM,Trinida & Tobago~T&T,Sydney Sixers~SS,Perth Scorchers~PS,Barbados Tridents~BT,Hobart Hurricanes~HH,Dolphins~DOL,Highveld Lions~HL,Titans~TIT,Yorkshire~YOR,Otago Volts~OV,Faisalabad Wolves~FW,Kandurata Maroons~KM,Brisbane Heat~BRH,Nepal~NEP,Northern Knights~NK,Southern Express~SP,Lahore Lions~LIO,Kolkata~KOL,Chennai~CHE,Rajasthan~RAJ,Hyderabad~HYD,Punjab~PUN,Bangalore~BLR,Mumbai~MUM,Delhi~DEL,Rising Pune Supergiants~RPS,Gujarat Lions~GL";
teamsstring=teamsstring.split(',');
var allTeamList = new Array();
var teamfullShort='';
for(x=0; x < teamsstring.length; x++){
	teamfullShort=teamsstring[x].split('~');
	allTeamList[teamfullShort[1]]=teamfullShort[0];
}
$(document).ready(function(){
	function get_cricket_live_widget(){
		var jsonUrl='http://ed-nm.ibnlive.in.com/livescore/json/crosslivescores.json';
		//var jsonUrl='crosslivescores.json';
		$.ajax({
			url: jsonUrl,
			type: 'GET',
			dataType: 'jsonp',
			jsonp: 'jsonp',
			jsonpCallback: 'scores',
			crossDomain: true,
			cache: false ,
			success:function(data){
				var widgetHtml='';
				var bulletsHtml='';
				var poweredBy='';
				var slideContainer='';
				var containerWidth='';
				var matchContent='';
				var livematchcount=data.matches.length;		
				
				for(var a=0; a < data.matches.length; a++){				
					var matchData=data.matches[a];
					var matchId=matchData.matchid;
					var matchStatus=matchData.status.toLowerCase();
					if(matchStatus.toLowerCase()=='stumps'){
						//continue;					
					}
					var matchDate = matchData.date;
					var matchNumber=matchData.matchno;					
					var seriesFullName=matchData.series_full.toLowerCase();
					seriesFullName=seriesFullName.replace(/,/g,'').replace(/ /g,"-").replace(/\//g,"-");					
					var teamAName=matchData.teama.name;
					var teamBName=matchData.teamb.name;
					if (typeof allTeamList[teamAName] != 'undefined' && typeof allTeamList[teamBName] != 'undefined'){
						var teamAFullName=allTeamList[teamAName].toLowerCase().split(' ').join('-');
						var teamBFullName=allTeamList[teamBName].toLowerCase().split(' ').join('-');
					}else{
						console.log("undefined error");						
						$("#matchResultBox").html('');
						return false;
					}
					
										
					var redirectUrl='/cricketnext/live-score/'+seriesFullName+'/'+teamAFullName+'-vs-'+teamBFullName+'/'+matchId+'.html';
								
					matchContent=get_active_inning(matchData);
					widgetHtml=widgetHtml+'<li><table><tr><td colspan="3" class="sdate">'+matchDate+' | <span class="sname">'+matchData.series+'</span></td></tr>'+matchContent+'</table><div class="toss">'+matchStatus.toUpperCase()+'</div><div class="fscore"><a href="'+redirectUrl+'">Full Scorecard</a></div></li>';
					
				}
							

				if(data.matches.length!=0){	
					$(".scoreWidgetTab li[data-slug=matchResultBox").addClass('act');
					$(".scoreWidgetTab li[data-slug=matchResultBox").attr('matchLength', data.matches.length);
					$("#matchResultBox").html(widgetHtml);
				}else{				
					$("#matchResultBox").html('<div class="noLive">No Live match</div>');
					$(".scoreWidgetTab li").removeClass('act');
					$(".scoreWidgetTab li[data-slug=RecentMatch").addClass('act');
					$('.scoreArrow').show();
					$(".scoreArrow").attr('tab_ul_id', "RecentMatch");
					$("#matchResultBox").hide();
					$("#RecentMatch").show();
				}			
				if(data.matches.length==0 && typeof widgetUpdate!='undefined'){
					$(".scoreWidgetTab li[data-slug=matchResultBox").attr('matchLength', data.matches.length);
					$(".scoreWidgetTab li[data-slug=RecentMatch").addClass('act');					
					$(".scoreArrow").attr('tab_ul_id', "RecentMatch");
					$("#matchResultBox").hide();
					$("#RecentMatch").show();
					$('.scoreArrow').show();
					clearInterval(widgetUpdate);
				}
				
				if(data.matches.length>1){
					$('.scoreArrow').show();
				}
				
			},
			error: function (error) {			
				$("#matchResultBox").html('');
			}
		});
	}
	function get_active_inning(matchData){
		var teamAScore=matchData.teama.scores;
		var teamAName=matchData.teama.name;	
		var teamBScore=matchData.teamb.scores;
		var teamBName=matchData.teamb.name;	
		var activeTeamName='';
		var activeTeamTotalRun='';
		var activeTeamWickets='';
		var activeTeamOvers='';
		var activeRunRate='';
		var activeStatus='';
		var matchNumber=matchData.matchno;
		var scoreAHtml='';
		var scoreBHtml='';
		// Get active inning team data
		for(var b=0; b < teamAScore.length; b++){
			activeStatus=teamAScore[b].active;
			if(activeStatus==1){
				activeTeamName=teamAScore[b].team;
				activeTeamTotalRun=teamAScore[b].scores;
				activeTeamWickets=teamAScore[b].wickets;
				activeTeamOvers=teamAScore[b].overs;
				activeRunRate=teamAScore[b].runrate;
			}
		}
		
		if(activeTeamName==''){
			for(var b=0; b < teamBScore.length; b++){
				activeStatus=teamBScore[b].active;
				if(activeStatus==1){
					activeTeamName=teamBScore[b].team;
					activeTeamTotalRun=teamBScore[b].scores;
					activeTeamWickets=teamBScore[b].wickets;
					activeTeamOvers=teamBScore[b].overs;
					activeRunRate=teamBScore[b].runrate;
				}
			}
		}
		
		if(activeTeamName!=''){
			var activeTeamImage='http://static.ibnlive.in.com/ibnlive/pix/ibnhome/cricketnext/microsite/teamsicon/'+activeTeamName.toLowerCase()+'.png';
				
			scoreAHtml='<td colspan="2">'+activeTeamTotalRun+'/'+activeTeamWickets+' <div class="overs">'+activeTeamOvers+' Overs</div></td>';
			scoreATdName='<td class="team-nam"><img src="'+activeTeamImage+'" alt="" height="18" width="27" /> '+activeTeamName+'</td>';
		}else{
			scoreATdName='<td class="team-nam"><img src="'+activeTeamImage+'" alt="" height="18" width="27" /> '+activeTeamName+'</td>';
			scoreAHtml='<td colspan="2">Yet to play</td>';
		}
		
		// Get second team Inning	
		var secongTeamName='';
		var secongTeamTotalRun='';
		var secongTeamWickets='';
		var secondTeamScore='';
		var secondTeamOver='';
		if(activeTeamName==teamAName){
			secondTeamScore=teamBScore;
			secongTeamName=teamBName;
		}else{
			secondTeamScore=teamAScore;
			secongTeamName=teamAName;
		}
		var secondTeamImage='http://static.ibnlive.in.com/ibnlive/pix/ibnhome/cricketnext/microsite/teamsicon/'+secongTeamName.toLowerCase()+'.png';		
		
		if(secondTeamScore.length > 0){
			for(var b=0; b < secondTeamScore.length; b++){
				secongTeamTotalRun=secondTeamScore[b].scores;
				secongTeamWickets=secondTeamScore[b].wickets;
				secondTeamOver = secondTeamScore[b].overs;
			}		
			scoreBHtml='<td>'+secongTeamTotalRun+'/'+secongTeamWickets+' <div class="overs">'+secondTeamOver+' Overs</div></td>';
			scoreBTdName='<td class="team-nam"><img src="'+secondTeamImage+'" alt="" height="18" width="27" /> '+secongTeamName+'</td>';
		}else{
			scoreBHtml='<td colspan="2">Yet to play</td>';
			scoreBTdName='<td class="team-nam"><img src="'+secondTeamImage+'" alt="" height="18" width="27" /> '+secongTeamName+'</td>';
		}	
		var matchDetailHtml = '<tr>'+scoreATdName+'<td class="vs">vs</td>'+scoreBTdName+'</tr><tr>'+scoreAHtml+scoreBHtml+'</tr>';
		
		return matchDetailHtml;
	}
	get_cricket_live_widget();
	var widgetUpdate=setInterval(function(){
		get_cricket_live_widget()
	}, 150000);
});
</script>
