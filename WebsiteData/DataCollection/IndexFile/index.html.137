					if ((!getCookie('welcomeAd') || !getCookie('dailyWelcomeCookie')) && !forbes.adblock_state) {
						document.cookie = "welcomeAd=true;path=/;domain=.forbes.com;expires=" + expiration;
						document.cookie = "dailyWelcomeCookie=true;path=/;domain=.forbes.com;expires=" + expiration;
					}
