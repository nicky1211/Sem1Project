   if(q_readCookie("v4iilex") != '')
    {
      ck_v4iilex = q_readCookie("v4iilex");
    }
    if((ck_v4iilex != "" && typeof ck_v4iilex != "undefined"))
    {
      loginMode = 1;
    }
    else if ((ck_ImeshVisitor != "" && typeof ck_ImeshVisitor != "undefined"))
    {
      loginMode = 2;
    }
    else
    {
      loginMode = 3;
    }
    if(loginMode == "undefined" || loginMode == '' || typeof loginMode == "undefined")
    {
      loginMode = 3;
    }
    
    return loginMode;
}

function nullBLField(tmplObj)
{
    $('[name="q_mobile"]').val('').attr('placeholder','Mobile').css('color','#acacac');
    $('[name="q_email"]').val('').attr('placeholder','Email').css('color','#acacac');

}

function isNumberKey(evt)
{
    var charCode = (evt.which);
    if(charCode == 118 && evt.ctrlKey === true)
    return true;
    if (charCode > 31 && (charCode < 48 || charCode > 57))
    return false;
    return true;
}

function isAlphaKey(evt)
{
   var keyCode = (evt.which);
   if((keyCode < 65 || keyCode > 90) && (keyCode < 97 || keyCode > 123) && keyCode != 32 && keyCode > 31)
   return false;
   return true;
}

