            if((typeof prod_serv != 'undefined') && (prod_serv != 'C'))
            {
		temp9Obj.formObj.q_title.value = temp9Obj.mcatName;
            }
	}
	
	if(temp9Obj.customHeadline != "")
	{
		temp9Obj.headStr = temp9Obj.customHeadline ;
	}
	
	if(temp9Obj.formObj.q_mcat.value != "")
	{
	  temp9Obj.mcatID = temp9Obj.formObj.q_mcat.value;
	}
	temp9Obj.formObj.q_category.value = temp9Obj.catID;
	temp9Obj.formObj.q_mcat.value = temp9Obj.mcatID;
	temp9Obj.formObj.q_modid.value = temp9Obj.modID;
	temp9Obj.formObj.afflid.value = temp9Obj.afflID;
	q_getCookieValues(temp9Obj.formObj);
	temp9Obj.formObj.q_desc.value = temp9Obj.placeholder._desc;
	
        getIPbasedCountry(temp9Obj);

	$(document).ready(function()
	{
	   if(!t9_isFlagSet)
            {
              temp9Obj.setFlagSuggester();
            }
            
            if(!t9_isTitleSuggSet)
            {
              temp9Obj.setTitleSuggester();
            }
	});
	
	function stopBgScroll()
	{
		$("html").css("overflow","hidden");
	}
	
	function resumeBgScroll()
	{
		$("html").css("overflow","auto");	
	}
	function bl_revert()
	{     
	      if(typeof isBLFormOpen != 'undefined')
	      {
	        isBLFormOpen = false;
              }
                intentGenerateFunc(temp9Obj);
		document.getElementById('t9_maindiv').style.display='none';
		resumeBgScroll();
	}
	function closeOnEsc()
	{
		$(document).keyup(function (e) {
			if(e.keyCode == 27)
			{
				if($("#t9_bglayer").css("display") == 'block')
				{
					bl_revert();
				}
			}
		});
	}
	function openOnClickBLForm(bltitle,heading)
	{
	     if(typeof isBLFormOpen != 'undefined')
	     {
	        isBLFormOpen = true;
             }
             intentGenerateFunc(temp9Obj);
             
             if(typeof bltitle == 'string')
             {
               temp9Obj.formObj.q_title.value = bltitle;
             }
             else
             {
               if(temp9Obj.mcatName !='')
                {
                  if((typeof prod_serv != 'undefined') && (prod_serv != 'C'))
                  {
                    temp9Obj.formObj.q_title.value = temp9Obj.mcatName;
                  }
                }
               else
               {
                 temp9Obj.formObj.q_title.value = "";
               }
             }
             
             if(heading)
             {
               document.getElementById('t9_heading').style.display='block';
               document.getElementById('t9_heading').innerHTML = heading;
             }
             else
             {
               document.getElementById('t9_heading').style.display='none';
             }
             
             if(document.getElementById('t9_maindiv').style.display='none')
              {
                   document.getElementById('t9_maindiv').style.display='table';
                   
                   if((temp9Obj.formObj.q_title.value != temp9Obj.placeholder._title) && (temp9Obj.formObj.q_title.value != ""))
                    {
                        if(temp9Obj.flags._isQtyOnFirstStep == true)
                        {
                           $("#t9_q_qty_enrichmt").focus();
                        }
                        else
                        {
                            $("#t9_q_desc").focus();
                        }
                     }
                    else
                     {
                        $("#t9_q_title").focus();
                     }
                    q_showMemReg(temp9Obj);
                    temp9Obj.setColor();
                    stopBgScroll();
                    closeOnEsc();
               }
            
            t9_loginMode = getLoginMode();
            var t9_track_event = '';
            
            if(typeof _gaq != "undefined" && _gaq != null)
            {
              _gaq.push(['_trackEvent', 'BL_Onclick_form', 'Form_Impression', temp9Obj.modID, 0, true]);
            
                if(t9_loginMode == 1)
                {
                    t9_track_event = 'Form_Impression_LoggedIn';
                }
                else if(t9_loginMode == 2)
                {
                    t9_track_event = 'Form_Impression_Identified';
                }
                else
                {
                    t9_track_event = 'Form_Impression_Not_Identified';
                }
                
                _gaq.push(['_trackEvent', 'BL_Onclick_form', t9_track_event , temp9Obj.modID, 0, true]);
            }
            else
            {
                document.getElementById('t9_q_lead_impressionload').innerHTML = '<iframe src="'+appsServerName+'blform/lead_impression.html?formName=BL_Onclick_form&modid='+temp9Obj.modID+'&trackevent='+t9_track_event+'"  width="0" height="0" border="0" frameborder="0"></iframe>';
            }
            
    }
    
function t9_bl_onSelect_countryISO(event, ui)
{ 
   $("#t9_Country_dropdown dt a span").attr('style','background-position:0px -'+ui.data.icon_order*11+'px');
 
   if(ui.value)
   {
      temp9Obj.formObj.q_country.value = ui.data.iso;
      temp9Obj.formObj.q_country_iso.value = ui.data.iso;
      temp9Obj.formObj.q_ph_country1.value = '+' + ui.value;
      temp9Obj.formObj.q_country_name.value = ui.data.cname;
      temp9Obj.formObj.iso.value = '+' + ui.value;
      if(ui.data.iso != 'IN')
      {
        document.getElementById('t9_indian_user').style.display = 'none';
        document.getElementById('t9_forgien_user').style.display = 'block';
        document.getElementById('t9_q_buyer_details').innerHTML = 'Email';
      }
      else
      {
	document.getElementById('t9_indian_user').style.display = 'block';
        document.getElementById('t9_forgien_user').style.display = 'none';
        document.getElementById('t9_q_buyer_details').innerHTML = 'Mobile';
      }
   
   }
   
}

function t9_bl_onSelect_title(event,ui)
{
  if(ui.item.value)
  {
    temp9Obj.formObj.q_title.value = ui.item.value;
    
    if(typeof _gaq != "undefined" && _gaq != null)
    {
      _gaq.push(['_trackEvent', 'BL_Onclick_form', 'AutoSuggest', temp9Obj.modID, 0, true]);
    }
    else
    {
      document.getElementById('t9_q_lead_impressionload').innerHTML = '<iframe src="'+appsServerName+'blform/lead_impression.html?formName=BL_Onclick_form&trackevent=AutoSuggest&modid='+temp9Obj.modID+'"  width="0" height="0" border="0" frameborder="0"></iframe>';
    }
  }
}
