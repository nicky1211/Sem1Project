    width: 100%;
}
.owl-controls .owl-pagination .owl-page {
    display: table-cell;
    zoom: 1;
    position: relative;
    top: 0px;
    width: 16.66%;
    vertical-align: top;
}
.owl-theme .owl-controls .owl-page span {
    display: block;
    margin:0px;
    line-height: 15px;
    color: #f9f9f9;
    font-family: "Roboto",sans-serif !important;
    font-size: 12px;

    font-weight: normal;
    padding: 5px 0px;
    position: relative;
    left: 0;
    right: 0;
    margin: 0 auto;
    position: relative;
    width: auto;
    height: auto;
    border-radius:0px;
    opacity: 1!important;
    background-color:transparent!important;
    text-transform: capitalize !important;
}
.owl-theme .owl-controls .owl-page span:after{
    position: absolute;
    top: 0px;
    right: 0px;
    content: '';
    display: inline-block;
    width: 1px;
    height: 100%;
    background: #ccc;
}
.owl-theme .owl-controls .owl-page:last-child span:after{
    display: none;
}
.owl-theme .owl-controls .owl-page.active span {
    color: #fff;
    background: #000 !important;
}
.owl-theme .owl-controls .owl-page.active span:after{
    display: none;
}


