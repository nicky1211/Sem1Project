        function customPager() {
        jQuery_1_10_2.each(this.owl.userItems, function (i) {
            var titleData = jQuery(this).find('img').attr('title');
		if(!titleData)
            		titleData = jQuery(this).find('div').attr('data-title');
		var imgAlt = jQuery(this).find('img').attr('alt');
            var dataUrl = jQuery(this).find('img').parent('a').attr('href');
            var paginationLinks = jQuery('.owl-controls .owl-pagination .owl-page span');
            jQuery_1_10_2(paginationLinks[i]).append(titleData);
            //jQuery_1_10_2(paginationLinks[i]).append(dataTitle);
            paginationLinks[i].setAttribute("title",titleData);
		if(imgAlt)
            		paginationLinks[i].setAttribute("alt",imgAlt);
            paginationLinks[i].setAttribute("id","HomebannerLabel"+(parseInt(i)+1));
            if(dataUrl){
              jQuery_1_10_2(paginationLinks[i]).wrap("<a href="+dataUrl+" target='_blank'></a>");
            }
        });

        }
