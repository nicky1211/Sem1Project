<script type="text/javascript" language="javascript">
/* Convonix Code Start */
    $(document).ready(function(){
	$("#checknetbanking").click(function(){
		CheckAjaxNetBankingNew();
	});
	
	function CheckAjaxNetBankingNew(){
		var mypath = "/json/jHandler.php";
		var netbBankingUrl = "https://www.kotak.com/j1001mp/netapp/MainPage.jsp";
		var redirectUrl = "/net_banking_site_down_MSG.html";
		var rString = randomString();
		$.ajax({
			type: "POST",
			url: mypath,
			dataType: 'Json',
			data: "op=" + rString + "&method=netbanking",
			success: function(Data){
				if(Data=="2"){
					$("#loginNetBanking").attr("href",redirectUrl);
					return true;
				}
				else{
					$("#loginNetBanking").attr("href",netbBankingUrl);
					return true;
				}
			},
			error: function(){
				$("#loginNetBanking").attr("href",netbBankingUrl);
				return true;
			}
		});
	}
	
    $('#BtnSendMobileLink').click(function(){
		var valid=true;
		if($("#countryCodeDD select").val()!="+91"){
			alert('Kindly select an Indian mobile number');
			valid=false;
		}else if(!(/^[0-9]{10}$/i.test($("#step1 #appmobileno").val()))){
			alert('Kindly enter a valid mobile number');
			valid=false;
		}else if($("#captchaimage").val() == ''){
			alert('Kindly enter captcha');
			valid=false;
		}else if($("#captchaimage").val() == 'Enter the characters shown in the image'){
			alert('Kindly enter captcha');
			valid=false;
		}else{
			if($('#step1 #appdownloadid').val() == "1"){
				dataLayer.push({  'category' : 'Download App',  'action' : 'Send me the link',  'label' : 'Kotak Mobile App',  'event' : 'event download app'  });
			}/*else if($('#step1 #appdownloadid').val() == "2"){
				dataLayer.push({  'category' : 'Download App',  'action' : 'Send me the link',  'label' : 'ING Vysya App',  'event' : 'event download app'  });
			}*/else if($('#step1 #appdownloadid').val() == "3"){
				dataLayer.push({  'category' : 'Download App',  'action' : 'Send me the link',  'label' : 'Kotak Bharat Banking',  'event' : 'event download app'  });
			}
			sendSMSNew($("#step1 #appmobileno").val(),$('#step1 #appdownloadid').val(),$("#captchaimage").val());
		}
		return false;
    });

	$('#downloadApp li').click(function(){
		mval = $(this).attr("value");
		$('#appdownloadid').val(mval);
		var rString = randomString();
		$("#img").attr("src", "/phpapps/xapi/captcha.php?str="+rString);
		$('#mobileBanking #step1').show();
		$('#mobileBanking #step2').hide();
		$("#step1 #appmobileno").val('');
		$("#captchaimage").val('Enter the characters shown in the image');
		$('#mobileBanking').show();
		//$('#downloadApp').val("");
    });

    $('#closeButton').click(function(){
		$(this).parent().hide();
    });
	
	
	function sendSMSNew(receiptant,message_code,sec_data){
		$.ajax({
			type: "POST",
			url: '/phpapps/xapi/smsxapi.php?sec_data='+sec_data+'&message_code='+message_code+'&mobile='+receiptant,
			data: "",
