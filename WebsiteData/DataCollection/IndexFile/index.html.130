<script type="text/javascript">
$(document).ready(function(){
	  $('li.check').each(function() {
	    var $dropdown = $(this);
	    $("i.menu", $dropdown).click(function(e) {
	      e.preventDefault();
	      $div = $("div.sub_drop_mob", $dropdown);
	      $div.toggle();
	      //$(this).html('Close');
	      var text = $(this).text();
	      $(this).text(text == "-" ? "+" : "-");
	      $("i.menu").not(this).html('+');
	      $("div.sub_drop_mob").not($div).hide();
	      return false;
	    });
	});
	  $('html').click(function(){
			var target = $(event.target);
			if(target.hasClass('check')||target.hasClass('menu')){
				$("div.sub_drop_mob").hide();
				var text = $(this).text();
				$("i.menu").text(text == "+" ? "-" : "+");
			}
		});
	});
</script>


