        <td width="17%" title="{enddate}" data-sortkey="{dateEndInt}">{enddate}</td>
    </tr>


</script> <script>

    $(document).ready(function ()
    {
        BuzzInTown.init();
    });

    var BuzzInTown = {
        config: {
            restaurants: {
                url: 'http://www.yatra.com/buzzintown/service/restaurant/',
                templateId: 'templateRows_restaurant',
                dataHolder: 'restaurants_data',
                params: {
                    type: 'top',
                    partner: 'yupg',
                    feed: 'json',
                    ci: null
                },
                dataKey: null
            },
            events: {
                url: 'http://www.yatra.com/buzzintown/service/events/',
                templateId: 'templateRows_events',
                dataHolder: 'events_data',
                params: {
                    type: 'upcoming',
                    partner: 'yourcode',
                    feed: 'json',
                    no: 15,
                    ci: null
                },
                dataKey: 'data'
            },
            tabSelector: '#buzztownTabs',
            citySelector: '#buzztownCity',
            moduleContainer: 'BuzzInTownThings',
            errorContainer: '#buzzTownErrorCont',
            tabContainer: '#buzztownContainer',
            cityList: [{name: 'Delhi (NCR)', value: '1'}, {name: 'Bangalore', value: '4'}, {name: 'Mumbai', value: '6'}]
        },
        constants: {
            serviceURL: appProp.ajaxServiceUrl,
            environment: appProp.site_environment_variable
        },
        isInit: false,
        init: function () {
            if (!this.isInit) {              // to execute only once
                this.isInit = true;
                this.makeUI();
                this.attachURLHandler(); // to open url in new tab
            }
            var url = this.makeAjaxURL();
            this.sendRequest(url);
        },
        sendRequest: function (url) {
            var $this = this;
            if (url && url.length > 0) {        //  if url exists and non-empty

                var ajaxSettings = {
                    url: url,
                    cache: true,
                    type: 'GET',
                    dataType: 'JSON',
                    success: function (data) {
                        $this.populate(data);
                        $this.hideErrorMsg();
                    },
                    error: function (e) {
//                        console.log(e);
                        $this.showErrorMsg();
                    }
                };
                if ($this.constants.environment == "local") {       // if local env. then use wscontent to get data(cross domain)
                    ajaxSettings.url = $this.constants.serviceURL;
                    ajaxSettings.type = 'POST';
                    ajaxSettings.data = {url: url};
                }

                MS_Loader.showLoader($this.getCenter(), "Please wait...", false);       // show loader during ajax call
                $.ajax(ajaxSettings);
            }
        },
        makeAjaxURL: function () {
            var url = '';
            var tabName = this.getActiveTabName();
            var cityCode = this.getCitySelected();

            if (tabName != null) {
                var params = this.config[tabName].params;       // taking parameters from mentioned params in config
                params.ci = cityCode;
                url = this.config[tabName].url + common_func.objToUrl(params); // converting object to url as get parameters
            }

            return url;
        },
        populate: function (data) {
            try {
                var tabName = this.getActiveTabName();
                var arr = data;
                var dataKey = this.config[tabName].dataKey;         // to get key which is containing the whole data(array)
                if (dataKey != null) {
                    arr = data[dataKey];
                }
                var template = new Template();  // templating html
                template.set(this.config[tabName].templateId);
                var html = "";

                for (var i = 0; i < arr.length; i++) {
                    // for making sorting key of dates, so as to sort data based on dates(for events category)
                    arr[i].dateStartInt = arr[i].startdate ? new Date(arr[i].startdate).getTime() : new Date().getTime();
                    arr[i].dateEndInt = arr[i].enddate ? new Date(arr[i].enddate).getTime() : new Date().getTime();
                    // clean data if keys are numbers so as to avoid errors in template class
                    clean(arr[i]);
                    html += template.render(arr[i]);
                }
                $('#' + this.config[tabName].dataHolder).html(html);
                this.updateScrollBar();
                CommonActions.refreshMySorter('#buzztownContainer .sorterBody');
                MS_Loader.hideLoader();
            }
            catch (e) {
                this.showErrorMsg();
//                console.log(e.message + " @ " + e.lineNumber + " on " + e.fileName);
            }

            function clean(d) {
                for (var i in d) {
                    if (parseInt(i) >= 0) {
                        delete d[i];        // remove key if it is integer
                    }
                }
                // handling url change, not getting right data through proxy
                if (typeof d.eventurl == "string") {
                    d.eventurl = d.eventurl.replace(/www.yatra.com/g, "www.buzzintown.com");
                }
                if (typeof d.venueurl == "string") {
                    d.venueurl = d.venueurl.replace(/www.yatra.com/g, "www.buzzintown.com");
                }
            }
        },
        makeUI: function ()
        {
            var $this = this;
            // Populating dropdown with available cities
            var cityList = this.config.cityList;
            $(this.config.citySelector).empty();
            for (var i = 0; i < cityList.length; i++) {
                $(this.config.citySelector).append('<option value="' + cityList[i].value + '">' + cityList[i].name + '</option>');
            }

            // Creating dropdown for selecting cities.
            $(this.config.citySelector).msDropDown({
                on: {
                    change: function () {
                        $this.init();
                        // GA
                        try {
                            var city = $($this.config.citySelector).children(':selected').text();
                            yt_ga.trackEventView(CommonActions.getProductSpecificEventCategory(), 'BuzzInTown', 'City Selected : ' + city);
                        } catch (e) {
//                            console.log(e);
                        }
                    },
                    open: CommonActions && CommonActions.scrollBarInDropdown
                }
            });
            // Creating tabs for events and restaurants content.
            $(this.config.tabSelector).msTabs({
                tabs: 'a',
                effects: 'none',
                selected: 'tripSel',
                callback: function () {
                    $this.init();
                    // do not send GA first time on init of tabbing(callabck id fired handled using flag)
                    if ($this.onceGA) {
                        try {
                            var tabName = $this.getActiveTabName();
                            yt_ga.trackEventView(CommonActions.getProductSpecificEventCategory(), 'BuzzInTown', 'Tab Selected : ' + tabName);
                        } catch (e) {
//                            console.log(e);
                        }
                    }
                    $this.onceGA = true;
                },
                toggle: false
            });
        },
        attachURLHandler: function () {
            $("#" + this.config.moduleContainer + ' [data-url]').live('click', function () {
                var url = $(this).data('url');
                window.open(url, '_blank');
//                window.location.href = url;
            });
        },
        getCitySelected: function () {
            return $(this.config.citySelector).val();
        },
        getActiveTabName: function () {
            try {
                var currId = $(this.config.tabSelector).data("msTabs").getCurrenTabId();
                var tabName = $('#' + currId).data('urlid');
                return tabName;
            }
            catch (e) {
                return null;
            }
        },
        getCenter: function () {
            var ele = $("#" + this.config.moduleContainer),
                    bufferWidth = 100,
                    top = (ele.height() / 2) + ele.offset().top,
                    left = (ele.width() / 2) + ele.offset().left - bufferWidth;
            return {top: top, left: left};
        },
        updateScrollBar: function () {
              CommonActions.enableCustomScroller(this.config.tabContainer+' .scrollContainer');
        },
        showErrorMsg: function () {
            $(this.config.errorContainer).show();
            $(this.config.tabContainer).hide();
            MS_Loader.hideLoader();
        },
        hideErrorMsg: function () {
            $(this.config.errorContainer).hide();
            $(this.config.tabContainer).show();
        },
        templateUtils: {
            parseprice: function (val) {
                if (val == "" || val == undefined) {
                    return ' style="padding-right:30px; text-align:right;" title="Price not available" data-sortkey="0">NA';
                }
                else {
                    return ' class="priceBlue price-right" title="Rs. '+ _RecentSearch_.Utils.formatNumber(val)+'" data-sortkey="' + val + '"><span class="rs">Rs. </span> ' + _RecentSearch_.Utils.formatNumber(val);
                }
            },
            parseDate: function (start, end) {
