</tr>
</script> <script id="templateRows_events" type="text/html">
    <tr data-url="{eventurl}" id="event_{event_id}" class="eventTrackable js-prodSpecEvtCat" data-trackcategory="Home Page" data-trackaction="BuzzInTown" data-trackvalue="Event: {name}" >
